{{#if (has-block)}}
  {{yield (hash handleChange=this.handleChange onDate=@onDate id=@id)}}
{{else}}
  {{#if @view}}
    {{#let (component @view) as |OnDateComponent|}}
      <OnDateComponent
        @id={{@id}}
        @onDate={{@onDate}}
        @handleChange={{this.handleChange}}
      />
    {{/let}}
  {{/if}}
{{/if}}