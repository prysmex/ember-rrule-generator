{{#if (has-block)}}
  {{yield
    (hash handleChange=this.handleChange onDate=@onDate id=@id name=@name)
  }}
{{else}}
  {{#if @view}}
    {{#let (component @view) as |OnDateComponent|}}
      <OnDateComponent
        @id={{@id}}
        id={{@id}}
        @name={{@name}}
        name={{@name}}
        @value={{@onDate}}
        @handleChange={{this.handleChange}}
      />
    {{/let}}
  {{/if}}
{{/if}}