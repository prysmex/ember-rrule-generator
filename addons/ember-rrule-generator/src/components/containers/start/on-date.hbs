{{#if (has-block)}}
  {{yield (hash handleChange=@handleChange onDate=@onDate id=@id)}}
{{else}}
  {{#if @view}}
    {{#let (component @view) as |OnDateComponent|}}
      <OnDateComponent
        @id={{@id}}
				id={{@id}}
				@name={{@name}}
				name={{@name}}
        @onDate={{@onDate}}
        @handleChange={{@handleChange}}
      />
    {{/let}}
  {{/if}}
{{/if}}