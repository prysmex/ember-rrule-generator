{{#let
  (hash
    OnDate=(component
      this.OnDate
      id=(concat @id "-onDate")
      onDate=@start.onDate
      handleChange=@handleChange
      name=(concat @name ".onDate.date")
      translations=@translations
    )
  )
  as |Start|
}}
  {{#if (has-block)}}
    {{yield Start}}
  {{else}}
    {{#let (component @view) as |StartComponent|}}
      <StartComponent @startApi={{Start}} ...attributes />
    {{/let}}
  {{/if}}
{{/let}}