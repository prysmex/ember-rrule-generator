{{#let
  (hash
    OnDate=(component
      this.OnDate
      id=(concat @id "-onDate")
      onDate=@start.onDate
      value=@start.onDate.date
      handleChange=@handleChange
      name=(concat @name ".onDate.date")
      translations=@translations
      labels=this.labels
      isDisabled=@isDisabled
    )
    isDisabled=@isDisabled
    labels=this.labels
  )
  as |Start|
}}
  {{#if (has-block)}}
    {{yield Start}}
  {{else}}
    {{#let (component @view) as |StartComponent|}}
      <StartComponent @Start={{Start}} ...attributes />
    {{/let}}
  {{/if}}
{{/let}}