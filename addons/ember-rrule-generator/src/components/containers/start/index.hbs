{{#if (has-block)}}
  {{yield
    (hash handleChange=this.handleChange value=@start.onDate.date id=@id)
  }}
{{else}}
  {{#if @view}}
    {{#let (component @view) as |CalendarComponent|}}
      <CalendarComponent
        @id={{@id}}
        @value={{@start.onDate.date}}
        @onChange={{this.handleChange}}
      />
    {{/let}}
  {{/if}}
{{/if}}