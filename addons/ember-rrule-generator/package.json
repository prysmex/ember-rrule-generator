{
  "name": "ember-rrule-generator",
  "description": "headless rrule generator",
  "version": "0.0.0",
  "private": false,
  "license": "MIT",
  "author": "betocantu93 <betocantu93@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:prysmex/ember-rrule-generator.git",
    "directory": "addons/ember-rrule-generator"
  },
  "homepage": "https://github.com/prysmex/ember-rrule-generator",
  "bugs": "https://github.com/prysmex/ember-rrule-generator/issues",
  "engines": {
    "node": "16.* || >= 18",
    "npm": "Use pnpm",
    "yarn": "Use pnpm"
  },
  "keywords": [
    "ember-addon"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@embroider/addon-shim": "^1.5.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "start": "npm-run-all --parallel start:*",
    "start:test-app": "cd ../../apps/test-app && ember serve",
    "start:build": "rollup --config --watch",
    "prepublishOnly": "rollup --config"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./components/containers/start/index.js": "./dist/_app_/components/containers/start/index.js",
      "./components/r-rule-generator/index.js": "./dist/_app_/components/r-rule-generator/index.js"
    },
    "public-assets": {}
  },
  "peerDependencies": {
    "dayjs": "^1.11.5",
    "lodash-es": "^4",
    "rrule": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/plugin-transform-typescript": "^7.18.8",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@embroider/addon-dev": "^1.6.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/chess.js": "^0.13.2",
    "@types/lodash-es": "^4.17.6",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.74.1",
    "rrule": "^2.7.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "walk-sync": "^3.0.0"
  },
  "ember": {
    "edition": "octane"
  }
}
